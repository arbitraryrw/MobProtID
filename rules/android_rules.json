{

    "rules": [
      {
        "ruleName": "rootdetectionFiles",
        "condition": "ALL",
        "part_1": {
            "type": "static",
            "signature": [
                "/sbin/su",
                "/system/bin/su",
                "/system/bin/failsafe/su",
                "/system/xbin/su"
            ]
        }

      },
      {
        "ruleName": "emulatorDetectionFiles",
        "condition": "ALL",
        "part_1": {
            "type": "static",
            "handler": "radare2",
            "signature": [
                "emulatorSignature1",
                "emulatorSignature2",
                "emulatorSignature3"
            ]
        },
          "part_2": {
            "type": "static",
            "handler": "yara",
            "signature": [
                "emulatorSignature4",
                "emulatorSignature5",
                "emulatorSignature6"
            ]
        }

      }
    ]
    
  }
