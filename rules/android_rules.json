{

    "rules": [
      {
        "ruleId":"001",
        "ruleName": "rootdetectionFiles",
        "description" : "Checks for the presence of the SU binaries statically",
        "condition": "AND",
        "part_1": {
            "type": "static",
            "handler": "yara",
            "signature": [
                "/sbin/su",
                "/system/bin/su",
                "/system/bin/failsafe/su",
                "/system/xbin/su"
            ]
        }

      },
      {
        "ruleId":"002",
        "ruleName": "emulatorDetectionFiles",
        "description" : "Checks for the prence of Emulator strings",
        "condition": "OR",
        "part_1": {
            "type": "static",
            "handler": "yara",
            "signature": [
                "emulatorSignature1",
                "emulatorSignature2",
                "emulatorSignature3"
            ]
        },
        "part_2": {
          "type": "static",
          "handler": "aaa",
          "signature": [
              "emulatorSignature4",
              "emulatorSignature5",
              "emulatorSignature6"
          ]
        }

      }

    ]
  }
